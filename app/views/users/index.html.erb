<h1>Users</h1>
<table>
  <thead>
    <% @users.each do |user| %>
    <% if user_signed_in? %>
     <td><%= link_to 'New User', new_user_registration_path %></td>
     <td><%= link_to 'Logout', destroy_user_session_path, :method => :delete %></td>
     <td><%= link_to 'Show Questions', questions_path %></td>
     <td><%= link_to 'Edit User', edit_user_registration_path(@user) %></td>
     <% else %>
     <td><%= link_to 'Login', new_user_session_path %></td>
   <% end %>
   <% end %>
 </thead>
</table>

<br>

